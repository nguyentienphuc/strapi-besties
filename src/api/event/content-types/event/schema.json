{
  "kind": "collectionType",
  "collectionName": "events",
  "info": {
    "singularName": "event",
    "pluralName": "events",
    "displayName": "Event",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "branchCode": {
      "type": "string",
      "required": false
    },
    "max": {
      "type": "integer",
      "max": 1000,
      "required": true
    },
    "start": {
      "type": "datetime",
      "required": true
    },
    "end": {
      "type": "datetime",
      "required": true
    },
    "description": {
      "type": "richtext",
      "required": true
    },
    "picture": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "event_users": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::event-user.event-user",
      "mappedBy": "event"
    },
    "votes": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::vote.vote",
      "mappedBy": "event"
    },
    "categoryCode": {
      "type": "string"
    },
    "rewardPoint": {
      "type": "integer"
    },
